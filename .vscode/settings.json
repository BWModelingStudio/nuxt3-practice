{
  "editor.formatOnSave": true,
  "prettier.semi": false,
  "files.trimTrailingWhitespace": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
  },
  "eslint.format.enable": true,
  "eslint.validate": ["javascript", "javascriptreact", "html", "vue", "typescript", "typescriptreact"],
  "eslint.enable": false,
  "eslint.alwaysShowStatus": true,
  "files.eol": "\n",
  "i18n-ally.displayLanguage": "zh-TW",
  "i18n-ally.localesPaths": "lang",
  "i18n-ally.enabledParsers": ["js", "ts", "json"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.sourceLanguage": "zh-TW",
  "terminal.integrated.inheritEnv": false
}
